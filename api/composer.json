{
  "name": "pronko/liqpay-api",
  "description": "LiqPay Api extension",
  "version": "1.2.0",
  "type": "magento2-module",
  "license": "proprietary",
  "authors": [
    {
      "name": "Pronko Consulting",
      "homepage": "https://www.pronkoconsulting.com/"
    }
  ],
  "minimum-stability": "alpha",
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "require": {},
  "require-dev": {
    "phpunit/phpunit": "~6.5",
    "magento/magento-coding-standard": "~2.0"
  },
  "scripts": {
    "test-phpcs": "vendor/bin/phpcs --standard=Magento2 etc/ Api/",
    "post-install-cmd": [
      "[ $COMPOSER_DEV_MODE -eq 1 ] && vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard/"
    ],
    "post-update-cmd": [
      "[ $COMPOSER_DEV_MODE -eq 1 ] && vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard/"
    ]
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Pronko\\LiqPayApi\\": ""
    }
  }
}
